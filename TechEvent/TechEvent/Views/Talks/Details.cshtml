@model TechEvent.Domain.Entities.Talk

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Talk</h4>
    <hr />
    <dl class = "row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Speaker)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Speaker.FullName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TalkType)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TalkType.Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-controller="Talks" asp-action="Index">Back to List</a>
</div>
